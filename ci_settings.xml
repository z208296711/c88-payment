<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
  <servers>
    <server>
      <id>gitlab-maven</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
  </servers>

  <profiles>
    <profile>
      <id>develop</id>
      <properties>
        <profileActive>develop</profileActive>
      </properties>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/156/packages/maven</url>
        </repository>
      </repositories>
      <distributionManagement>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/156/packages/maven</url>
        </repository>
        <snapshotRepository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/156/packages/maven</url>
        </snapshotRepository>
      </distributionManagement>
    </profile>

    <profile>
      <id>pre</id>
      <properties>
        <profileActive>pre</profileActive>
      </properties>
      <repositories>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/174/packages/maven</url>
        </repository>
      </repositories>
      <distributionManagement>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/174/packages/maven</url>
        </repository>
        <snapshotRepository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/174/packages/maven</url>
        </snapshotRepository>
      </distributionManagement>
    </profile>

    <profile>
      <id>qa</id>
      <properties>
        <profileActive>qa</profileActive>
      </properties>
      <repositories>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/176/packages/maven</url>
        </repository>
      </repositories>
      <distributionManagement>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/176/packages/maven</url>
        </repository>
        <snapshotRepository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/176/packages/maven</url>
        </snapshotRepository>
      </distributionManagement>
    </profile>

    <profile>
      <id>stg</id>
      <properties>
        <profileActive>stg</profileActive>
      </properties>
      <repositories>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/177/packages/maven</url>
        </repository>
      </repositories>
      <distributionManagement>
        <repository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/177/packages/maven</url>
        </repository>
        <snapshotRepository>
          <id>gitlab-maven</id>
          <url>https://gitlab.hygo.cc/api/v4/projects/177/packages/maven</url>
        </snapshotRepository>
      </distributionManagement>
    </profile>
  </profiles>

  
</settings>
